{
  "name": "layercode-dual-channel",
  "version": "1.0.0",
  "description": "Layercode dual channel voice agent setup",
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:channel-a": "NEXT_PUBLIC_CHANNEL_ID=channel_a next dev --turbopack --port 3000",
    "dev:channel-b": "NEXT_PUBLIC_CHANNEL_ID=channel_b next dev --turbopack --port 3001",
    "build": "next build",
    "start": "next start",
    "start:channel-a": "NEXT_PUBLIC_CHANNEL_ID=channel_a next start --port 3000",
    "start:channel-b": "NEXT_PUBLIC_CHANNEL_ID=channel_b next start --port 3001",
    "lint": "next lint",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "2.0.27",
    "@layercode/node-server-sdk": "1.2.1",
    "@layercode/react-sdk": "^2.3.0",
    "@opennextjs/cloudflare": "1.9.0",
    "ai": "5.0.54",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "zod": "4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@tailwindcss/postcss": "4.1.13",
    "@types/node": "20.19.19",
    "@types/react": "19.1.16",
    "@types/react-dom": "19.1.9",
    "eslint": "9.34.0",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "4.1.13",
    "typescript": "5.9.3"
  }
}
